<table width="100%" border="0" cellspacing="0" cellpadding="3">
    <tr bgcolor="#000000">
        <td width="9%" align="center">
            <stripes:link beanclass="pm.matthews.webmail.action.BaseAction" event="main">
                <img src="images/main_small.png" width="20" height="20" border="0" />
            </stripes:link>
        </td>
        <td width="3%" align="center">
            <stripes:link beanclass="pm.matthews.webmail.action.BaseAction" event="compose">
                <img src="images/compose_small.png" width="20" height="20" border="0" />
            </stripes:link>
        </td>
        <td width="10%" align="center">
            <stripes:link beanclass="pm.matthews.webmail.action.ContactAction" event="displayContacts">
                <img src="images/addresses_small.png" width="20" height="20" border="0" />
            </stripes:link>
        </td>
        <td width="7%" align="center">
            <stripes:link beanclass="pm.matthews.webmail.action.PreferenceAction" event="displayPreference">
                <img src="images/settings_small.png" width="20" height="20" border="0" />
            </stripes:link>
        </td>
        <td width="6%" align="center">
            <stripes:link beanclass="pm.matthews.webmail.action.FolderAction" event="logout">
                <img src="images/logout_small.png" width="21" height="20" border="0" />
            </stripes:link>
        </td>
        <td width="65%" bgcolor="#000000">&nbsp;</td>
    </tr>
    
    <tr bgcolor="#dadada">
        <td width="9%" align="center" nowrap="">
            <stripes:link beanclass="pm.matthews.webmail.action.BaseAction" event="main">
                <font face="Arial, Helvetica, sans-serif" size="-1"><b><fmt:message key="menu.tomain"/></b></font>
            </stripes:link>
        </td>
        <td width="3%" align="center" nowrap="">
            <stripes:link beanclass="pm.matthews.webmail.action.BaseAction" event="compose">
            <font face="Arial, Helvetica, sans-serif" size="-1"><b><fmt:message key="menu.compose"/></b></font>
            </stripes:link>
        </td>
        <td width="10%" align="center" nowrap="">
            <stripes:link beanclass="pm.matthews.webmail.action.ContactAction" event="displayContacts">
                <font face="Arial, Helvetica, sans-serif" size="-1"><b><fmt:message key="menu.contacts"/></b></font>
            </stripes:link>
        </td>
        <td width="7%" align="center" nowrap="">
            <stripes:link beanclass="pm.matthews.webmail.action.PreferenceAction" event="displayPreference">
                <font face="Arial, Helvetica, sans-serif" size="-1"><b><fmt:message key="menu.settings"/></b></font>
            </stripes:link>
        </td>
        <td width="6%" align="center" nowrap="">
            <stripes:link beanclass="pm.matthews.webmail.action.FolderAction" event="logout">
                <font face="Arial, Helvetica, sans-serif" size="-1"><b><fmt:message key="menu.logout"/></b></font>
            </stripes:link>
        </td>
        <td width="65%" nowrap=""><font color="#00000">&nbsp;</font></td>
    </tr>
</table>
<br></br>
